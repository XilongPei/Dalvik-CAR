
public class Main{
	public static void main(String[] args)
	{
		System.out.println("test start here\n");
		CTestBasicType test = new CTestBasicType();

		System.out.println("test boolean:");
		boolean bi=true;
		boolean bj=false;
		System.out.println("!" + bi + " = " + test.boolNon(bi));
		System.out.println( bi + " && " + bj + " = " + test.boolAnd(bi,bj));
		System.out.println( bi + " || " + bj + " = " + test.boolOr(bi,bj));
		System.out.println();


		System.out.println("test String:");
		String si="hello world";
		String sj="hello";

		System.out.println( "copy string, result = " + test.strCopy(si));

		System.out.println( "connect two string, result = "+ test.strCon(si,sj));

		System.out.println( "Does " + si + " start with " + sj + "? = " + test.strStartwith(si,sj));
		System.out.println();

	}
}
